{
  "name": "@agenc/sdk",
  "version": "1.2.0",
  "description": "TypeScript SDK for AgenC - Privacy-preserving agent coordination on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "bin": {
    "agenc": "./dist/bin/agenc.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/bin/agenc.ts --format cjs,esm --clean --external privacycash",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "privacy",
    "zk",
    "zero-knowledge",
    "noir",
    "agents",
    "coordination",
    "escrow",
    "privacy-cash"
  ],
  "author": "Tetsuo AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tetsuo-ai/AgenC"
  },
  "homepage": "https://github.com/tetsuo-ai/AgenC#readme",
  "bugs": {
    "url": "https://github.com/tetsuo-ai/AgenC/issues"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.0",
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@coral-xyz/anchor": ">=0.29.0",
    "@solana/web3.js": ">=1.90.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
