# Environment variables for llm-agent example
# Copy to .env and fill in values before running.

SOLANA_RPC_URL=https://api.devnet.solana.com
KEYPAIR_PATH=~/.config/solana/id.json

# Provider selection (first match wins): XAI_API_KEY > ANTHROPIC_API_KEY > Ollama
XAI_API_KEY=
ANTHROPIC_API_KEY=

# Model overrides
GROK_MODEL=grok-3
ANTHROPIC_MODEL=claude-sonnet-4-5-20250929
OLLAMA_MODEL=llama3

